{
  "expo": {
    "name": "tugas-akhir",
    "slug": "tugas-akhir",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tugasakhir",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alvin.widi.tugasakhir"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed5e7a30-4012-4d53-b286-cea2d6688071"
      }
    }
  }
}
